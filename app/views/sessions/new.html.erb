<% provide(:title, 'Log in') %>
<div class = "row">
	<div class = "large-4 medium-6 columns small-centered">
		<h1>Log in</h1>
		<%= form_for(:session, url: login_path) do |f| %>
		

		<%= f.label :email %>
		<%= f.email_field :email %>
		

		<%= f.label :password %>
		<%= f.password_field :password %>

		<%= f.submit "Log in", class: "button success radius" %>
		
		<p>New user? <%= link_to "Sign up now!", signup_path %></p>
		<% end %>
	</div>
</div>


